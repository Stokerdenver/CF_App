<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Dis_1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Dis_1"
    xmlns:view="clr-namespace:Dis_1.View"
    Shell.FlyoutBehavior="Disabled"
    Title="Dis_1">

    <TabBar>
        <Tab Title="Личный кабинет">
            <ShellContent ContentTemplate="{DataTemplate local:PersonalAccount}" Route="PersonalAccount"/>
        </Tab>
        <Tab Title="GPS Кнопка">
            <ShellContent ContentTemplate="{DataTemplate view:GPS_page}" Route="GPS_page" />
        </Tab>
        <Tab Title="GPS Авто">
            <ShellContent ContentTemplate="{DataTemplate local:GPS_test}" Route="GPS_test" />
        </Tab>
    </TabBar>

</Shell>
